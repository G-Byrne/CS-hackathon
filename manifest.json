{
  "manifest_version": 3,
  "name": "Tab Birthdays",
  "description": "Celebrate that special day for your special tabs",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ]
}